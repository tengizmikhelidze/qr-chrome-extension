{
  "manifest_version": 3,
  "name": "QR Generator",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_title": "QR Generator",
    "default_icon": "favicon.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ]
}
